<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Store_Phone_Number__c</fullName>
    <description>This pulls in the Home Store&#39;s phone number. It is used on email templates</description>
    <externalId>false</externalId>
    <formula>IF(
NOT(ISBLANK(Assigned_Store__c)),
Assigned_Store__r.Phone_Number__c,
Home_Store__r.Phone_Number__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Store Phone Number</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
