<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_Must_Be_In_The_Future</fullName>
    <active>true</active>
    <description>Start Date must be in the future.</description>
    <errorConditionFormula>ISNEW() &amp;&amp; Start_Date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date must be in the future.</errorMessage>
</ValidationRule>
